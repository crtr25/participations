<Window x:Class="football_schedule.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:football_schedule"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox x:Name="lsbAway" HorizontalAlignment="Left" Height="231" Margin="342,57,0,0" VerticalAlignment="Top" Width="135"/>
        <ListBox x:Name="lsbHome" HorizontalAlignment="Left" Height="231" Margin="212,57,0,0" VerticalAlignment="Top" Width="125"/>
        <TextBox x:Name="txtHome" HorizontalAlignment="Left" Height="26" Margin="212,26,0,0" TextWrapping="Wrap" Text="Home Games" VerticalAlignment="Top" Width="112" />
        <TextBox x:Name="txtAway" HorizontalAlignment="Left" Height="26" Margin="342,26,0,0" TextWrapping="Wrap" Text="Away Games" VerticalAlignment="Top" Width="112"/>
        <ComboBox x:Name="cmbYear" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="25,57,0,0"/>
        <TextBox x:Name="txtSelect" HorizontalAlignment="Left" Height="26" Margin="25,26,0,0" TextWrapping="Wrap" Text="Select a year" VerticalAlignment="Top" Width="112" />

    </Grid>
</Window>
